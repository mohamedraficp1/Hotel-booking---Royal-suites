
 

  <!-- dashboard inner -->
               <div class="midde_cont">
                  <div class="container-fluid">
                     <div class="row column_title">
                        <div class="col-md-12">
                           <div class="page_title">
                              <h2>Users</h2>
                           </div>
                        </div>
                     </div>
                   

                     <div class="row">
                        <!-- table section -->
                

                         <div class="col-md-9">
                           <div class="white_shd full margin_bottom_30">
                              <div class="full graph_head">
                                 <div class="heading1 margin_0">
                                    <h2>Verified users</h2>
                                 </div>
                              </div>
                              <div class="table_section padding_infor_info">
                                 <div class="table-responsive-sm">
                                    <table class="table table-dark">
                                       <thead>
                                         <tr>
                                           <th>Sl No</th>
                                             <th>Name</th>
                                            <th>Email</th>
                                            <th>Phone Number</th>
                                            <th>Bock User</th>
                                          </tr>
                                       </thead>
                                       <tbody>
                                           {{#each response}}
                                          <tr>
                                             <td>{{inc @index}}</td>
                                            <td>{{this.name}}</td>
                                            <td>{{this.email}}</td>
                                            <td>{{this.phoneNumber}}</td>
                                                <td><a  href="/admin/block-user/{{this._id}}" class="btn btn-danger d-inline-block">Block</a>
                                            </td>
                                          </tr>
                                         {{/each}}
                                         
                                       </tbody>
                                    </table>
                                 </div>
                              </div>
                           </div>
                        </div>

                         <div class="col-md-9">
                           <div class="white_shd full margin_bottom_30">
                              <div class="full graph_head">
                                 <div class="heading1 margin_0">
                                    <h2>Blocked users</h2>
                                 </div>
                              </div>
                              <div class="table_section padding_infor_info">
                                 <div class="table-responsive-sm">
                                    <table class="table table-dark">
                                       <thead>
                                         <tr>
                                           <th>Sl No</th>
                                             <th>Name</th>
                                            <th>Email</th>
                                            <th>Phone Number</th>
                                            <th>UnBock User</th>
                                          </tr>
                                       </thead>
                                       <tbody>
                                           {{#each users}}
                                          <tr>
                                             <td>{{inc @index}}</td>
                                            <td>{{this.name}}</td>
                                            <td>{{this.email}}</td>
                                            <td>{{this.phoneNumber}}</td>
                                                <td><a  href="/admin/unblock-user/{{this._id}}" class="btn btn-success d-inline-block">Un Block</a>
                                            </td>
                                          </tr>
                                         {{/each}}
                                         
                                       </tbody>
                                    </table>
                                 </div>
                              </div>
                           </div>
                        </div>
                      
    
               </div>
               <!-- end dashboard inner -->
            </div>
 